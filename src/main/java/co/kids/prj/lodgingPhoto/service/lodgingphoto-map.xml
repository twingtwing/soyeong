<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.kids.prj.lodgingPhoto.service.LodgingPhotoMapper">
	<select id="selectListPhoto">
		SELECT * FROM IMG
	</select>
	
	<select id="selectPhoto">
	
	</select>
	
	<insert id="insertPhoto" parameterType="co.kids.prj.reservation.service.ReservLodVO">
		INSERT INTO IMG(IMGNO, RNO, ID, ORIFILE, PFILE) VALUES(
		(SELECT MAX(imgno) FROM img)+1, ${rno}, ${id}, ${orifile}, ${pfile}
		)
		
	</insert>
	
	<update id="updatePhoto">
	
	</update>
	
	<delete id="deletePhoto">
	
	</delete>

</mapper>